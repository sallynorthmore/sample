{
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"modules": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"prettier/prettier": "warn",
		"default-case":"error",
		"react/sort-comp": "warn"
	},
	"globals": {
		"module": true,
		"global": true,
		"process": true
	}
}
